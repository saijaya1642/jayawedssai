name: Deploy to Firebase Hosting on PR
'on': 
    push:
        branches: [Test]
    pull_request:
        branches: [Test]
jobs:
  build:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v2
      - uses: FirebaseExtended/action-hosting-deploy@v0
        with:
          repoToken: '${{ secrets.GITHUB_TOKEN }}'
          firebaseServiceAccount: '${{ secrets.FIREBASE_SERVICE_ACCOUNT_AUTH_DEVELOPMENT_33CC6 }}'
          projectId: auth-development-33cc6
          channelId: live
